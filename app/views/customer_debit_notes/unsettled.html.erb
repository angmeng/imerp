<br />
<%= link_to "Back", customer_debit_notes_path %>
<br/>
<%= will_paginate @customer_debit_notes %>
<br />
<table class="list">
  <tr>

    <th>Customer Debit note date</th>
    <th>Customer Debit note number</th>
    <th>Customer</th>
    <th>Voided</th>
    <th>Posted</th>
    <th>Action</th>
    <th></th>
    <th></th>
  </tr>

<% @customer_debit_notes.each do |customer_debit_note| %>
  <tr class="<%= cycle "odd", "even"  %> " style="text-align: center">

    <td><%= customer_debit_note.cust_debit_note_date %></td>
    <td><%= customer_debit_note.cust_debit_note_number %></td>
    <td><%= customer_debit_note.customer.name rescue "-" %></td>
    <td><%= customer_debit_note.voided %></td>
    <td><%= customer_debit_note.posted %></td>
    <td><%= link_to 'Edit', edit_customer_debit_note_path(customer_debit_note) %>
    | <%= link_to 'Destroy', customer_debit_note, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<%= will_paginate @customer_debit_notes %>
<br />
