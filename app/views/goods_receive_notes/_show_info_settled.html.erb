<div class="mg10 exhibiton ex01">
    <div class="exHead">
        <div class="hTxt">
            <div class="hTxtContent"><h2>Good Receive Note Details</h2></div>
        </div>
        <div class="hLeft"></div>
        <div class="hRight"></div>
    </div>
    <div class="exBody">
        <div class="content">
            <table class="list">
              <tr>
                <th>GRN No</th>
                <th>Date</th>
                <th>S/N</th>
                <th>DO No</th>
                <th>Invoice No</th>
                <th>PO No</th>
                <th>Supplier</th>
                <th>Status</th>
              </tr>
              <tr align="center">
                <td><%=h @goods_receive_note.grn_number %></td>
                <td><%=h @goods_receive_note.grn_date %></td>
                <td><%=h @goods_receive_note.serial_number %></td>
                <td><%=h @goods_receive_note.invoice_number %></td>
                <td><%=h @goods_receive_note.do_number %></td>
                <td><%=h @goods_receive_note.purchase_order.purchase_order_number %></td>
                <td><%=h @goods_receive_note.supplier.name %></td>
               <% if @goods_receive_note.voided? %>
                 <td><%=raw @goods_receive_note.void_status %></td>
              <%else%>
                 <td><%=raw @goods_receive_note.status %></td>
               <%end%>
              </tr>
            </table>
            <table class="list">
              <tr align="left">
               <th align="left">Remark</th>
            </tr>
            <tr align="left">
            <td colspan="8"> <%= @goods_receive_note.remark %></td>
            </tr>
            </table>
 </div>
   </div>
    <div class="exFoot">
        <div class="fLeft"></div>
        <div class="fRight"></div>
    </div>
</div>


