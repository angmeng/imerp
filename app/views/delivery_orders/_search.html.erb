<div class="mg10 exhibiton ex01">
    <div class="exHead">
        <div class="hTxt">
          <div class="hTxtContent"><h2>DO Search</h2></div>
        </div>
        <div class="hLeft"></div>
        <div class="hRight"></div>
    </div>
    <div class="exBody">
        <div class="content">

<%= form_for @search do |f| %>
  <table class="list" style="width: auto">
    <tr>
      <th>DO No</th>
      <th>PO No</th>
      <th>Product Code</th>
       <th>Product Description</th>
       <th></th>

    </tr>
    <tr>
      <td align="center"><%= f.text_field :delivery_order_number_like %></td>    
      <td align="center"><%= f.text_field :purchase_order_number_like %></td>
     <td align="center"><%= f.text_field :delivery_order_items_product_code_like %></td>
     <td align="center"><%= f.text_field :delivery_order_items_product_full_description_like %></td>

   </tr>
   <tr>
    
     <th>Packing List No</th>
      <th>Customer</th>
      <th>DO Status</th>
      <th>Void Status</th>
      <th>Action</th>
   </tr>
   <tr>
     
      <td align="center"><%= f.text_field :packing_list_packing_list_number_like %></td>
      <td align="center"><%= f.select :customer_id_equals, Customer.all(:order => "name").map {|c| [c.name, c.id]} , {:include_blank => true} %></td>
      <td align="center"><%= f.select :settled_equals, [["Completed", true],["Processing", false]], {:include_blank => true} %></td>
       <td align="center"><%= f.select :voided_equals, [["Voided", true],["Unvoided", false]], {:include_blank => true} %></td>
      <td align="center"><%= f.submit "Search" %></td>
   </tr>
</table>
   </div>
   </div>
    <div class="exFoot">
        <div class="fLeft"></div>
        <div class="fRight"></div>
    </div>
</div>

<% end %>