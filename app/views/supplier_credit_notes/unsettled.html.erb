<br />
<%= link_to "Back", supplier_credit_notes_path %>
<br/><%= will_paginate @supplier_credit_notes %>
<br />
<table class="list">
  <tr>
    <th>Supplier Credit note number</th>
    <th>Supplier Credit note date</th>
    <th>Supplier</th>
    <th>Status</th>
    <th>Voided</th>
    <th>Posted</th>
    <th>Action</th>
    <th></th>
    <th></th>
  </tr>

<% @supplier_credit_notes.each do |supplier_credit_note| %>
  <tr class="<%= cycle "odd", "even"  %> " style="text-align: center">
    <td><%= supplier_credit_note.supplier_credit_note_number %></td>
    <td><%= supplier_credit_note.supplier_credit_note_date %></td>
    <td><%= supplier_credit_note.supplier.name rescue "-" %></td>
    <td align="center"><%=raw supplier_credit_note.status %></td>
    <td><%= supplier_credit_note.voided %></td>
    <td><%= supplier_credit_note.posted %></td>
    <td><%= link_to 'Edit', edit_supplier_credit_note_path(supplier_credit_note) %>
    | <%= link_to 'Destroy', supplier_credit_note, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<%= will_paginate @supplier_credit_notes %>
<br />