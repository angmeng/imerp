
  <div class="mg10 exhibiton ex01">
    <div class="exHead">
        <div class="hTxt">
            <div class="hTxtContent"><h2>Purchase Order Details</h2></div>
        </div>
        <div class="hLeft"></div>
        <div class="hRight"></div>
    </div>
    <div class="exBody">
        <div class="content">
                <table class="list">
                  <tr>
                    <th>PO No</th>
                    <th>Supplier</th>
                    <th>Action</th>
                  </tr>
                <% @purchase_requisition.purchase_orders.each do |po| %>
                  <tr class="<%= cycle("odd", "even") %>" align="center">
                    <td><%= po.purchase_order_number   %></td>
                    <td><%= po.supplier.name   %></td>
                    <td><%= link_to "show", po  %></td>
                  </tr>
                <% end %>
                </table>
        </div>
   </div>
    <div class="exFoot">
        <div class="fLeft"></div>
        <div class="fRight"></div>
    </div>
</div>
