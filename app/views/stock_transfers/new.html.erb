<% content_for :head do %>
<%= javascript_include_tag("smo") %>
<% end %>
<%= render "/shared/autocomplete" %>

<% title 'New Stock Transfer'  %>
<br />
<%= link_to 'Back', stock_transfers_path %>
<br /><br />

 <%= form_for(@stock_transfer) do |f| %>
  <%= render "shared/error_messages", :target => @stock_transfer %>
<table>
      <%= f.hidden_field :user_id, :value => current_user.id %>
      <tr>
        <th>Date</th><td><%= f.date_select :stock_transfer_date %></td>
      </tr>
      <tr>
        <th>Remark</th><td valign="top"><%= f.text_area :remark, :size => "60x2" %></td>
      </tr>


 </table>
  <p>
    <%= f.submit 'Next' %>
  <% end %>
  </p>

